{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Clawb AI",
  "colors": {
    "primary": "#e11d48",
    "light": "#fb7185",
    "dark": "#be123c"
  },
  "favicon": "/favicon.png",
  "navigation": {
    "tabs": [
      {
        "tab": "Docs",
        "groups": [
          {
            "group": "Getting started",
            "pages": [
              "index",
              "quickstart"
            ]
          },
          {
            "group": "Agents",
            "pages": [
              "agents/overview",
              "agents/attestation"
            ]
          },
          {
            "group": "Policies",
            "pages": [
              "policies/overview"
            ]
          },
          {
            "group": "Integration",
            "pages": [
              "integration/check",
              "integration/examples"
            ]
          },
          {
            "group": "SDK",
            "pages": [
              "sdk/python"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Clawb",
          "href": "https://clawb.ai",
          "icon": "globe"
        },
        {
          "anchor": "App",
          "href": "https://app.clawb.ai",
          "icon": "grid"
        },
        {
          "anchor": "GitHub",
          "href": "https://github.com/clawbai",
          "icon": "github"
        }
      ]
    }
  },
  "navbar": {
    "links": [
      {
        "label": "Clawb.ai",
        "href": "https://clawb.ai"
      },
      {
        "label": "GitHub",
        "href": "https://github.com/clawbai"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Open dashboard",
      "href": "https://app.clawb.ai/login"
    },
    "logo": {
      "type": "image",
      "image": "/images/logo.png",
      "alt": "Clawb",
      "height": 24
    }
  },
  "contextual": {
    "options": [
      "copy",
      "view",
      "chatgpt",
      "claude",
      "perplexity",
      "mcp",
      "cursor",
      "vscode"
    ]
  },
  "footer": {
    "socials": {
      "x": "https://x.com/joinclawb",
      "github": "https://github.com/clawbai"
    }
  }
}
